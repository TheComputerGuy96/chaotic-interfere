
source+=('config.arch')
sha256sums+=('SKIP')

_suffix="${_config#config_x86-64}"
_suffix="${_suffix#-v1}"

pkgname=("${pkgbase}${_suffix}" "${pkgbase}-headers${_suffix}")
for _p in "${pkgname[@]}"; do
  _p=${_p%$_suffix}
  eval "package_$_p$_suffix() {
    $(declare -f "_package${_p#$pkgbase}")
    _package${_p#$pkgbase}
  }"
done
