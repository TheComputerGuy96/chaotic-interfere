
export MAKEFLAGS="${MAKEFLAGS:-} -j $(nproc)"

# package options
_microarchitecture=91
_config=config_x86-64-v1

use_numa=n
use_tracers=n
#_compiler=clang

_compress_modules=y
_localmodcfg=n
_makenconfig=n

# pkgbase extension
_pkgtype=${_config#config_x86-64}
__pkgtype=${_compiler:+-$_compiler}${_pkgtype#-v1}
